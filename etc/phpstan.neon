        - '#Method [a-zA-Z0-9\\_]+::errorsArray\(\) has parameter \$violations with no value type specified in iterable type [a-zA-Z0-9\\_]+.#'
        - '#Method [a-zA-Z0-9\\_]+::errorsArray\(\) return type has no value type specified in iterable type array.#'
        - '#Property [a-zA-Z0-9\\_]+::\$violations type has no value type specified in iterable type [a-zA-Z0-9\\_]+.#'
        - '#Method [a-zA-Z0-9\\_]+::__construct\(\) has parameter \$violations with no value type specified in iterable type [a-zA-Z0-9\\_]+.#'
        - '#Method [a-zA-Z0-9\\_]+::getViolations\(\) return type has no value type specified in iterable type [a-zA-Z0-9\\_]+#'
