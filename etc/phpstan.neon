parameters:
    level: 8
    paths:
      - ../src
      - ../tests
      - ../public
      - ../configuration
      - ../bin
      - ../migrations
    ignoreErrors:
        - '#Method [a-zA-Z0-9\\_]+::filterStrings\(\) return type has no value type specified in iterable type array.#'
